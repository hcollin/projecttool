{
	"extends": "../../../tsconfig.base.json",
	"compilerOptions": {
		"moduleResolution": "node10",
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"target": "ES2022",
		"useDefineForClassFields": true,
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
