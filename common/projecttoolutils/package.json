{
    "name": "@frosttroll/projecttoolutils",
    "version": "0.1.0",
    "type": "commonjs",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "dist/src/index.d.ts",
    "exports": {
        "./": "./",
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --config vite.config.ts",
        "clean.ps": "Remove-Item -Recurse -Force ./dist",
        "clean.sh": "rm -rf ./dist",
        "clean.cmd": "rmdir /s /q .\\dist"
    },
    "devDependencies": {
        "@frosttroll/projecttoolmodels": "*",
        "@types/node": "^24.10.1",
        "typescript": "~5.9.3",
        "vite": "^7.2.2",
        "vite-plugin-dts": "^4.5.4"
    },
    "dependencies": {
        "rndlib": "^0.9.3"
    }
}