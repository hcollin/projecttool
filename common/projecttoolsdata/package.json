{
    "name": "@frosttroll/projecttooldata",
    "version": "0.1.0",
    "type": "commonjs",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
