    },
    "devDependencies": {
        "@frosttroll/projecttoolmodels": "*",
        "@types/node": "^24.10.1",
        "typescript": "~5.9.3",
        "vite": "^7.2.2",
        "vite-plugin-dts": "^4.5.4"
    },
    "dependencies": {
        "rndlib": "^0.9.3"
    }
}